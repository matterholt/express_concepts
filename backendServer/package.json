{
  "name": "babel_setup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node ./src/index.js",
    "server": "node ./dist-server/index.js",
    "transpile": "babel src  --out-dir dist-server",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "clean": "rimraf dist-server",
    "build": "npm-run-all clean transpile",
    "test": "jest",
    "watch:dev": "nodemon",
    "buildDB": "babel-node src/utils/testModel.js"
  },
  "author": "MAtterholt",
  "license": "ISC",
  "devDependencies": {
    "@babel/node": "^7.10.5",
    "eslint": "^6.7.1",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "@babel/cli": "^7.11.5",
    "@babel/core": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sequelize": "^6.3.5",
    "sqlite3": "^5.0.0",
    "uuid": "^3.3.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*",
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
